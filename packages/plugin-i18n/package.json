{
  "name": "@ice/plugin-i18n",
  "version": "1.0.0",
  "description": "The i18n plugin for icejs 3.",
  "license": "MIT",
  "type": "module",
  "main": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "default": "./esm/index.js"
    },
    "./runtime": {
      "types": "./esm/runtime/index.d.ts",
      "import": "./esm/runtime/index.js",
      "default": "./esm/runtime/index.js"
    },
    "./runtime/I18nContext": {
      "types": "./esm/runtime/I18nContext.d.ts",
      "import": "./esm/runtime/I18nContext.js",
      "default": "./esm/runtime/I18nContext.js"
    },
    "./esm/runtime/I18nContext": {
      "types": "./esm/runtime/I18nContext.d.ts",
      "import": "./esm/runtime/I18nContext.js",
      "default": "./esm/runtime/I18nContext.js"
    },
    "./createI18nMiddleware": {
      "types": "./esm/createI18nMiddleware.d.ts",
      "import": "./esm/createI18nMiddleware.js",
      "default": "./esm/createI18nMiddleware.js"
    },
    "./esm/createI18nMiddleware": {
      "types": "./esm/createI18nMiddleware.d.ts",
      "import": "./esm/createI18nMiddleware.js",
      "default": "./esm/createI18nMiddleware.js"
    }
  },
  "files": [
    "esm",
    "!esm/**/*.map"
  ],
  "repository": {
    "type": "http",
    "url": "https://github.com/ice-lab/ice-next/tree/master/packages/plugin-fusion"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc"
  },
  "dependencies": {
    "accept-language-parser": "^1.5.0",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@ice/types": "^1.0.0",
    "@types/react": "^18.0.21",
    "history": "^5.3.0",
    "react-router-dom": "^6.4.1",
    "webpack-dev-server": "^4.11.1"
  }
}